<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" 
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">

  <profiles>
    <profile>
     <id>develop-pod-profile</id>
     <properties>
       <symphony-develop-bot1-identity>
         {
           "privateKey": "${env.BOT1PRIVATEKEY}",
           "email": "symphony.practice.bot1@list.db.com",
           "commonName": "symphony.practice.bot1"
         }
       </symphony-develop-bot1-identity>
       <symphony-develop-bot2-identity>
         {
           "privateKey": "${env.BOT2PRIVATEKEY}",
           "email": "symphony.practice.bot2@list.db.com",
           "commonName": "symphony.practice.bot2",
           "chain": [ "${env.BOT2CERTIFICATE}" ]
         }
       </symphony-develop-bot2-identity>
       <symphony-develop-bot3-identity>
         {  
           "chain": [ "${env.BOT3CERTCHAIN}" ],
           "privateKey": "${env.BOT3PRIVATEKEY}",
           "publicKey": "${env.BOT3PUBLICKEY}",
           "email":"symphony.practice.bot3@list.db.com",
           "commonName":"bob",
           "password" : "${env.BOT3P12PASSWORD}"
         }
       </symphony-develop-bot3-identity>
       <symphony-develop-bot4-identity>
         {  
           "chain": [ "${env.BOT4CERTCHAIN}" ],
           "email":"symphony.practice.bot4@list.db.com",
           "commonName":"symphony.practice.bot4"
         }
       </symphony-develop-bot4-identity>
       <symphony-test-identity>
         {
           "privateKey": "${env.BOT1PRIVATEKEY}",
           "email": "symphony.practice.bot1@list.db.com",
           "commonName": "symphony.practice.bot1"
         }
       </symphony-test-identity>
     </properties>
    </profile>
  </profiles>
  <activeProfiles>
   <activeProfile>develop-pod-profile</activeProfile>
   <activeProfile>symphony-ci</activeProfile>
  </activeProfiles>
</settings>
